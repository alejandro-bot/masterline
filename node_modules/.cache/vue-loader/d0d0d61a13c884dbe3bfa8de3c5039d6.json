 vs-justify=\"center\" vs-align=\"center\" vs-lg=\"6\" vs-sm=\"6\" vs-xs=\"12\">\r\n                      <select\r\n                        class=\"seleccionable ml-1\"\r\n                        v-model=\"formAssign.branch_office_id\"\r\n                        style=\"width: 100%; margin-top: 10px; padding: 15px; margin-bottom: 20px;\"\r\n                      >\r\n                        <option value=\"\">Seleccione</option>\r\n                        <option\r\n                          :key=\"index\"\r\n                          :value=\"item.id\"\r\n                          v-for=\"(item, index) in branchOffices\"\r\n                        >\r\n                          {{ item.name }}\r\n                        </option>\r\n                      </select>\r\n                  </vs-col>\r\n                  <vs-col vs-type=\"flex\" vs-justify=\"center\" vs-align=\"center\" vs-lg=\"12\" vs-sm=\"12\" vs-xs=\"12\">\r\n                     <vs-button\r\n                        @click=\"assignRo()\"\r\n                        color=\"#ff5000\"\r\n                        type=\"relief\"\r\n                        icon=\"add\"\r\n                        >Asignar R.O</vs-button\r\n                      >\r\n                  </vs-col>\r\n                </vs-row>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n              </div>\r\n            </vs-card>\r\n          </vs-col>\r\n        </vs-row>\r\n      </vs-col>\r\n    </vs-row>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { dominio } from \"../dominio.js\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: this.$route.params && this.$route.params.id,\r\n      users: [],\r\n      branchOffices: [],\r\n      formAssign: {\r\n        user_id: \"\",\r\n        branch_office_id: \"\",\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.showUser();\r\n    this.showBranchOffices();\r\n  },\r\n  methods: {\r\n    returnRo() {\r\n      this.$router.push(\"/panel/show-ro\");\r\n    },\r\n    showUser()